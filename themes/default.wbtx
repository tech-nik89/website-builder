<theme>
	<description>
		This is a default theme included in the project.
		Icons from https://icons8.com are used.
	</description>
	<settings>
		<imageCssClass>.img-{0}</imageCssClass>
	</settings>
	<style type="less" name="default">
		@acentColor: #DD5533;
		
		html, body {
			margin:0;
			background-color:#DDDDDD;
		}
		
		body {
			font-family: Verdana;
			font-size:9pt;
		}
		
		header {
			background-color:#303030;
			color:#FFF;
			padding:20px;
			text-transform:uppercase;
			font-size:2em;
			
			.img-logo {
				vertical-align:middle;
				padding-left:15px;
			}
		}
		
		h1, h2, h3, h4, h5, h6 {
			text-transform:uppercase;
			color:@acentColor;
			font-weight:normal;
			padding:4px;
			margin:0;
		}
		
		h1 {
			font-size:1.5em;
		}
		
		h2 {
			font-size:1.3em;
		}
		
		h3 {
			font-size:1.2em;
		}
		
		main {
			border-top:2px solid @acentColor;
			background-color:#FFF;
			margin:20px;
			padding:10px;
			box-shadow: 0 2px 2px #AAA;
		}
		
		.languages {
			position:absolute;
			top:10px;
			right:10px;
			font-size:9pt;
			height:22px;
			width:22px;
			overflow:hidden;
			text-transform:none;
			border: 2px solid transparent;
			outline:none;
			
			> i {
				position:absolute;
				top:4px;
				right:4px;
				cursor:pointer;
			}
			
			&amp;:hover {
				height:auto;
				width:auto;
				background-color:#292929;
				border:2px solid @acentColor;
				border-radius:3px;
			}
			
			> ul {
				padding:0;
				margin:28px 0 0 0;
				
				> li {
					margin:0;
					padding:0;
					
					a {
						display:block;
						margin:0;
						padding:4px 10px;
						
						&amp;:link, &amp;:visited {
							color:#FFF;
							text-decoration:none;
						}
						
						&amp;:hover, &amp;:active {
							background-color:@acentColor;
						}
					}
				}
			}
		}
		
	</style>
	<style type="less" name="navigation">
		@acentColor: #DD5533;
		@bgColor: #292929;
		@border: 3px solid @acentColor;
		
		nav {
			background-color:@bgColor;
			border-bottom:@border;
			text-transform:uppercase;
			
			a {
				padding:10px 20px;
				
				&amp;:link, &amp;:visited {
					color:#FFF;
					text-decoration:none;
					display:block;
				}
				
				&amp;:hover, &amp;:active {
					background-color:@acentColor;
				}
			}
			
			ul {
				list-style-type:none;
				background-color:@bgColor;
				padding:0;
				margin:0;
			}
			
			> ul {
				margin:0 15px;
				padding:0;
				
				> li {
					position:relative;
					display:inline-block;
					
					> ul {
						position:absolute;
						left:0;
						top:35px;
						display:none;
						width:100%;
						border-top:@border;
						z-index:1;
						
						li {
							position:relative;
							
							ul {
								display:none;
								position:absolute;
								left:100%;
								top:0;
							}
						}
					}
				}
				
				li:hover {
					> ul {
						display:block;
					}
				}
			}
		}
	</style>
	<image name="logo">	
		
	</image>
	<image name="languages">
		
	</image>
	<layout title="Simple" class="" sections="1">
		@Model.Content[0]
	</layout>
	<layout title="Two Columns" class="two-columns" sections="2">
		<section>
			@Model.Content[0]
		</section>
		<section>
			@Model.Content[1]
		</section>
	</layout>
	<navItems>
		<ul>
			@Model.Children
		</ul>
	</navItems>
	<navItem>
		<li>
			<a href="@Model.Url">@Model.Text</a>
			@Model.Children
		</li>
	</navItem>
	<languageItems>
		<span class="languages">
			<i class="img-languages"></i>
			<ul>
				@Model.Items
			</ul>
		</span>
	</languageItems>
	<languageItem>
		<li>
			<a href="@Model.Url">@Model.Text</a>
		</li>
	</languageItem>
	<body>
		<header>
			<i class="img-logo"></i>
			@Model.Title
			@Model.Languages
		</header>
		<nav>
			@Model.Navigation
		</nav>
		<main>
			@Model.Content
		</main>
	</body>
</theme>